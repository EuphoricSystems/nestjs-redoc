{
  "name": "@u4ic/nestjs-redoc",
  "version": "2.3.0",
  "description": "NestJS ReDoc frontend",
  "repository": "https://github.com/euphoricsystems/nestjs-redoc",
  "author": "Chris Young",
  "main": "dist/index.js",
  "private": false,
  "license": "MIT",
  "scripts": {
    "bootstrap": "yarn && yarn prepare",
    "prebuild": "rimraf ./dist",
    "build": "tsc -p ./tsconfig.build.json",
    "format": "prettier --write \"./src/**/*.ts\"",
    "format:check": "prettier --check \"./src/**/*.ts\"",
    "lint": "eslint \"./src/**/*.ts\" --fix",
    "prepublish:next": "yarn run build",
    "publish:next": "yarn publish --access public --tag next",
    "prepublish": "yarn run build",
    "publish": "yarn publish --access public",
    "test": "jest",
    "test:watch": "jest --watch",
    "sample": "ts-node ./sample/main"
  },
  "peerDependencies": {
    "@nestjs/common": "8.4.5",
    "@nestjs/core": "8.4.5",
    "@nestjs/swagger": "6.3.0",
    "reflect-metadata": "0.1.13"
  },
  "dependencies": {
    "@hapi/joi": "17.1.1",
    "@nestjs/common": "8.4.5",
    "@nestjs/swagger": "6.3.0",
    "express-basic-auth": "1.2.1",
    "express-handlebars": "6.0.6"
  },
  "devDependencies": {
    "@commitlint/cli": "9.1.2",
    "@commitlint/config-conventional": "9.1.2",
    "@nestjs/core": "8.4.5",
    "@nestjs/platform-express": "8.4.5",
    "@nestjs/platform-fastify": "8.4.5",
    "@nestjs/testing": "8.4.5",
    "@types/express": "4.17.13",
    "@types/express-handlebars": "3.1.0",
    "@types/hapi__joi": "17.1.8",
    "@types/jest": "26.0.24",
    "@types/node": "14.18.18",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "commitizen": "4.2.4",
    "eslint": "7.32.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-prettier": "3.4.1",
    "husky": "4.3.8",
    "jest": "26.6.3",
    "prettier": "2.6.2",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "6.6.7",
    "supertest": "6.2.3",
    "ts-jest": "26.5.6",
    "ts-node": "9.1.1",
    "typescript": "4.6.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
